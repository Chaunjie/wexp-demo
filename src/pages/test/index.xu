<template>
  <view bindtap="tapPro" class="{{num < 3 ? 'test' : 'name'}}">{{text}}</view>
</template>
<script>
  import wexp from 'wexp/index'
  export default class Test extends wexp.page {
    config = {
      'navigationBarTitleText': '测试页面'
    }

    data = {
      text: 'chaunjie',
      num: 2
    }

    onLoad (options) {
      const name = this.$parent.globalData.name
      this.setData({
        text: name
      })
      console.log(name)
    }

    tapPro (e) {
      this.$back()
    }
  }
</script>